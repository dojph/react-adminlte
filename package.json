  "license": "MIT",
  "repository": "dojph/react-adminlte",
  "homepage": "http://dojph.github.io/react-adminlte/",
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Justin Dane Vallar",
    "email": "jdvallar@gmail.com",
    "url": "https://github.com/vallarj"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "adminlte",
    "react-adminlte",
    "react",
    "components"
  ],
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "react-popper": "^2.2.5",
    "react-resize-aware": "^2.7.2",
    "react-select": "^4.3.1",
    "react-smooth-collapse": "^2.1.0",
    "react-transition-group": "^4.4.2"
  },
  "scripts": {
    "prestart": "npm run gen:docs",
    "start": "concurrently npm:start:docs npm:gen:docs-watch",
    "start:docs": "craco start",
    "gen:docs": "node scripts/generate-component-data.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "prebuild:docs": "npm run gen:docs",
    "build:docs": "craco build",
    "predeploy:docs": "npm run build:docs",
    "deploy:docs": "gh-pages -d build",
    "test": "craco test --env=jsdom",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all build:commonjs build:css build:copy-files",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
    "build:css": "copyfiles -u 2 \"./src/components/**/*.css\" ./lib",
    "build:copy-files": "node scripts/copy-build-files.js",
    "eject": "react-scripts eject"
  },
  "peerDependencies": {
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@craco/craco": "^6.2.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "concurrently": "^6.2.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-docgen": "^5.4.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-syntax-highlighter": "^15.4.4",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
